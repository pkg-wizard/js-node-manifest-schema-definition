{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "regulatoryFields",
	"type": "object",
	"title": "Regulatory Fields",
	"description": "Regulatory Fields",
	"properties": {
		"dataPrivacy": {
			"type": "string",
			"enum": [
				"public",
				"private"
			]
		},
		"customerInfo": {
			"type": "string",
			"enum": [
				"PII",
				"nonPII"
			]
		},
		"dataOrigin": {
			"type": "string",
			"enum": [
				"machineGenerated",
				"humanGenerated"
			]
		},
		"monetaryInfo": {
			"type": "string",
			"enum": [
				"financialData",
				"nonFinancialData"
			]
		}
	}
}
